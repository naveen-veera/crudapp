<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginController</servlet-name>
  	<display-name>LoginController</display-name>
  	<description></description>
  	<servlet-class>crudapp.controller.LoginController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SignupController</servlet-name>
  	<display-name>SignupController</display-name>
  	<description></description>
  	<servlet-class>crudapp.controller.SignupController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Signupcontroller</servlet-name>
  	<display-name>Signupcontroller</display-name>
  	<description></description>
  	<servlet-class>crudapp.controller.Signupcontroller</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginController</servlet-name>
  	<url-pattern>/LoginController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SignupController</servlet-name>
  	<url-pattern>/SignupController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Signupcontroller</servlet-name>
  	<url-pattern>/Signupcontroller</url-pattern>
  </servlet-mapping>
</web-app>
